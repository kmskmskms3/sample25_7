package  {	import flash.display.MovieClip;	import flash.text.TextField;	import flash.text.TextFormat;	import flash.text.TextFormatAlign;	import flash.events.MouseEvent;	import flash.events.EventDispatcher;	import flash.ui.Mouse;	import flash.events.Event;		public class Btn extends EventDispatcher{		public static const CUSTOM_EVENT:String = "Btn.CUSTOM_EVENT";		private var _parent:MovieClip;		private var _rect:MovieClip;		private var _mozi:TextFormat;		public var _btnFace:String;				public function get BTNFACE():String {return _btnFace;}				public function Btn(p:MovieClip,x:uint,y:uint,tx:String):void {			_parent = p;			_btnFace = tx;			_rect = new MovieClip();			_mozi = new TextFormat();			_mozi.font = "_ゴシック";			_mozi.size = 18;			_mozi.align = TextFormatAlign.CENTER;			_mozi.color = 0x333333;			var mfield : TextField = new TextField();			mfield.defaultTextFormat = _mozi;			mfield.width = 150;			mfield.text = _btnFace;			mfield.x = x;			mfield.y = y;			mfield.mouseEnabled = false;						_rect.graphics.beginFill(0xFF0000,0.6);			_rect.graphics.drawRect(x, y, 150,30);			_rect.graphics.endFill();			_rect.buttonMode = true;			_rect.addEventListener(MouseEvent.CLICK, checkEvent);						_parent.addChild(_rect);			_parent.addChild(mfield);		}				 private function checkEvent (e:MouseEvent):void{			dispatchEvent(new Event(CUSTOM_EVENT));				//trace("a");		}	}}